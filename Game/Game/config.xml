<!-- Config file for the game -->

<config>
  <app>
    <title>Ninja Frog Against Gravity</title>
    <organization>Nuto Studios + UPC</organization>
  </app>
	
  <renderer>
    <vsync value="true"/>
  </renderer>

  <audio>
    <volume value="64"></volume>
  </audio>

  <window>
    <resolution width="1024" height="768" scale="2.5"/>
    <fullscreen value="false"/>
    <borderless value="false"/>
    <resizable value="false"/>
    <fullscreen_window value="false"/>
  </window>

  <map>
    <folder>maps/</folder>
  </map>
  <player>
    <player_data>
      <position></position>
      <desired_position></desired_position>
      <size></size>
      <velocity></velocity>
      <gravity></gravity>
      <collider></collider>
      <feet_position></feet_position>
      <feet_collider></feet_collider>
      <s_down></s_down>
      <grounded></grounded>
      <has_doublejumped></has_doublejumped>
      <mirror></mirror>
      <alive></alive>
    </player_data>
    <animations>
      <idle></idle>
      <jump></jump>
      <moving></moving>
    </animations>
  </player>

  <scene current_scene ="1"/>

</config>